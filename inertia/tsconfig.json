{
  "extends": "@adonisjs/tsconfig/tsconfig.client.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsxImportSource": "@emotion/react",
    "module": "ESNext",
    "jsx": "react-jsx",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["../*"]
    },
    "types": ["vite/client", "@emotion/styled", "@emotion/react"]
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
