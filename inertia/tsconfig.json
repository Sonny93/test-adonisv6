{
  "extends": "@adonisjs/tsconfig/tsconfig.client.json",
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "jsx": "react-jsx",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "../*"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx"
  ]
}