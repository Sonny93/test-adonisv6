    "@emotion/styled": "^11.13.0",
    "@inertiajs/react": "^1.0.15",
    "@types/uuid": "^9.0.8",
    "@vinejs/vine": "^2.0.0",
    "dayjs": "^1.11.10",
    "edge.js": "^6.0.2",
    "framer-motion": "^11.3.30",
    "luxon": "^3.4.4",
    "mediasoup": "^3.14.1",
    "mediasoup-client": "^3.7.6",
